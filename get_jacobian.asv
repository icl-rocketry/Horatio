function [A, B] = get_jacobian(x, u, epsilon_x, epsilon_u)
    zero_vec = zeros(length(x));
    
    for i = range(length(x))
        eps_x = zero_vec;
        eps_u = zero_vec;
        eps_x(i) = epsilon_x(i);
        eps_u(i) = epsilon_u(i);

        A = (dynamics_fn(x + eps_x, u) - dynamics_fn(x + eps_x, u)) / (2 * eps_x(i));
        B = (dynamics_fn(x, u + eps_u) - dynamics_fn(x, u + eps_u)) / (2 * eps_u(i));
    end 


end